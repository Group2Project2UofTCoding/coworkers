<img id="logo" src="./assets/images/logo.png" alt="Coworkers Logo" width="500">

<form id="signInForm">
  {{!-- <!-- Email input --> --}}
  <div class="form-group">
    <label for="exampleInputEmail1">Email</label>
    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email" required>
    <small id="emailHelp" class="form-text text-muted">You'll use this email id for signing in</small>
  </div>
  {{!-- <!-- Password input --> --}}
  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password" required>
  </div>
    {{!-- <!-- Submit form data --> --}}
  <div class="d-grid gap-2 mx-auto">
    <button id="signUp" type="submit" class="btn btn-primary mt-3 w-100">Sign Up</button>
  </div>
<div>
    <a href="/login">Login instead</a>
</div>

<script src="/javascript/login.js"></script>